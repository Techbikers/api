{
  "name": "techbikers.com",
  "version": "2.0.0",
  "description": "The techbikers.com webite for ride administration, rider registration",
  "author": "Michael Willmott <mwillmott@gmail> (http://mwillmott.co)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Techbikers/techbikers/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Techbikers/techbikers.git"
  },
  "scripts": {
    "start": "better-npm-run watch",
    "start-server": "python manage.py runserver 8000 --settings=server.settings.local",
    "build": "better-npm-run build",

    "test-live-api": "npx dredd docs/swagger.yaml https://techbikers.com/",
    "test-local-api": "npx dredd docs/swagger.yaml http://localhost:8000/",

    "create-db": "python manage.py migrate --settings=server.settings.local",
    "populate-db": "python manage.py createdummydata --settings=server.settings.local",
    "create-admin": "python manage.py createsuperuser --settings=server.settings.local",

    "lint": "eslint client"
  },
  "betterScripts": {
    "watch": {
      "command": "webpack-dev-server --inline --hot --history-api-fallback --config config/webpack.dev.config.js --port 3000"
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config config/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "auth0-js": "8.5.0",
    "babel-polyfill": "6.13.0",
    "classnames": "2.2.5",
    "compass-mixins": "0.12.10",
    "dredd": "^5.1.8",
    "http-status": "0.2.3",
    "humps": "2.0.0",
    "intl": "1.2.4",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.0",
    "js-cookie": "2.1.2",
    "jwt-decode": "2.1.0",
    "lodash": "4.14.2",
    "moment": "2.14.1",
    "newforms": "0.13.2",
    "normalize.css": "6.0.0",
    "normalizr": "^3.2.2",
    "polished": "^1.0.4",
    "raven-js": "3.4.0",
    "react": "15.3.0",
    "react-document-title": "2.0.2",
    "react-dom": "15.3.0",
    "react-intl": "2.1.3",
    "react-modal": "1.4.0",
    "react-redux": "5.0.3",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "redux": "3.6.0",
    "redux-actions": "1.2.2",
    "redux-localstorage": "0.4.1",
    "redux-logger": "2.6.1",
    "redux-router": "2.1.2",
    "redux-saga": "0.14.3",
    "redux-segment": "1.5.0",
    "reselect": "2.5.3",
    "styled-components": "^1.4.4"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.11.6",
    "better-npm-run": "0.0.10",
    "core-decorators": "0.12.3",
    "css-loader": "0.23.1",
    "eslint": "3.2.2",
    "eslint-loader": "1.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "glob": "7.1.1",
    "json-loader": "0.5.4",
    "node-sass": "3.8.0",
    "path": "0.12.7",
    "postcss": "5.1.2",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "2.7.0",
    "postcss-each": "0.9.3",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.9.1",
