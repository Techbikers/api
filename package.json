    "postcss-url": "5.1.2",
    "react-hot-loader": "1.3.0",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "1.14.1",
    "webpack-postcss-tools": "1.1.2"
  }
}
